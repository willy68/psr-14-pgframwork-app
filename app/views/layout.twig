					class="navbar-nav mr-auto">
					{#}	<li class="nav-item">
											<a class="nav-link" href="{{ path('blog.index') }}">Blog</a>
										</li>
										<li class="nav-item">
											<a class="nav-link" href="{{ path('admin') }}">Administration</a>
										</li> #}
					{{ blog_menu() }}
					<li class="nav-item dropdown">
						<a class="nav-link dropdown-toggle" href="#" id="dropdown01" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Dropdown</a>
						<div class="dropdown-menu" aria-labelledby="dropdown01">
							<a class="dropdown-item" href="template.html">Template.html</a>
							<a class="dropdown-item" href="{{ path('demo.react') }}">Demo React</a>
							<a class="dropdown-item" href="{{ path('demo.index') }}">Something else here</a>
						</div>
					</li>
				</ul>
				{#}
				        {% if current_user() %}
				          <div class="navbar-testn mr-2 text-white">{{ current_user().username }}</div>
									<form class="form-inline mr-2 my-2 my-lg-0" action="{{ path('demo.index') }}" method="POST">
