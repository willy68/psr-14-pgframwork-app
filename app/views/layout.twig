										<button class="btn btn-secondary mr-2 mr-sm-0" type="submit">Se deconnecter</button>
									</form>
								{% else %}
									<ul class="navbar-nav ml-auto">
										<li class="nav-item">
											<a class="nav-link" href="{{ path('demo.index') }}">Se connecter</a>
										</li>
									</ul>
								{% endif %}
								#}
				{#<form class="form-inline my-2 my-lg-0">
									<input class="form-control mr-sm-2" type="text" placeholder="Search" aria-label="Search">
									<button class="btn btn-secondary my-2 my-sm-0" type="submit">Search</button>
								</form>#}
			</div>
		</nav>
		<main role="main" class="container">

			{% if flash('success') %}

				<div class="alert alert-success">
					{{ flash('success') }}
				</div>

			{% endif %}

			{% if flash('error') %}

				<div class="alert alert-danger">
					{{ flash('error') }}
				</div>

			{% endif %}

			{% block body %}{% endblock %}

		</main>
		<!-- /.container -->

		<script src="/assets/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
